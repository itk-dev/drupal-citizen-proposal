{
    "name": "itk-dev/drupal_citizen_proposal",
    "description": "Citizen proposal",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "description": "Citizen proposal",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "keywords": [
        "Drupal"
    ],
    "require": {
        "php": "^8.3",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/ds": "^3.20",
        "drupal/entity_events": "^2.0",
        "drupal/field_group": "^3.4",
        "drupal/symfony_mailer": "^1.4",
        "drupal/twig_tweak": "^3.3",
        "drupal/view_custom_table": "^2.0",
        "drupal/views_data_export": "^1.4",
        "drupal/webform": "^6.2",
        "drupal/xls_serialization": "^1.3",
        "drush/drush": "^12.5",
        "itk-dev/drupal_psr6_cache": "^1.1",
        "itk-dev/openid-connect": "^3.1",
        "itk-dev/serviceplatformen": "^1.5"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "drupal/coder": "^8.3",
        "drupal/maillog": "^1.1",
        "ergebnis/composer-normalize": "^2.42",
        "mglaman/phpstan-drupal": "^1.2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^12"
            }
        }
    },
    "scripts": {
        "code-analysis": [
            "@code-analysis/phpstan"
        ],
        "code-analysis/phpstan": [
            "phpstan analyse"
        ],
        "coding-standards-apply": [
            "@coding-standards-apply/phpcs"
        ],
        "coding-standards-apply/phpcs": [
            "phpcbf --standard=phpcs.xml.dist"
        ],
        "coding-standards-check": [
            "@coding-standards-check/phpcs"
        ],
        "coding-standards-check/phpcs": [
            "phpcs --standard=phpcs.xml.dist"
        ]
    }
}
